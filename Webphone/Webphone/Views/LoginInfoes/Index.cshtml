@model IEnumerable<Webphone._Models.LoginInfo>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_LayoutMn.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.UserName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Passw)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LoginRole)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LoginStatus)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Passw)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LoginRole)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LoginStatus)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.LoginId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.LoginId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.LoginId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
